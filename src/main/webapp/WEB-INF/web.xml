<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
         http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>FuelServlet</servlet-name>
        <servlet-class>ErenUgur.Servlet.FuelServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>FuelServlet</servlet-name>
        <url-pattern>/FuelLog</url-pattern>
    </servlet-mapping>
<servlet>
    <servlet-name>GPSServlet</servlet-name>
    <servlet-class>ErenUgur.Servlet.GPSServlet</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>GPSServlet</servlet-name>
    <url-pattern>/gps</url-pattern>
</servlet-mapping>
</web-app>